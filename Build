def build

def instancia(buildInfo, groovyBase){
     if (buildInfo.tipoProjeto == 'maven'){
         sh "wget ${_groovyBase}/BuildMaven.groovy"
         codeBuildMaven = load 'BuildMaven.groovy'
         build          = codeBuildMaven.instancia(buildInfo)
     }else if (buildInfo.tipoProjeto == 'node'){ 
          sh "wget ${_groovyBase}/BuildNode.groovy"
          codeBuildNode = load 'BuildNode.groovy'
          build          = codeBuildNode.instancia(buildInfo)
     }
     return this
}

def build(){
     if (isMaven){
         sh " mvn clean install -f ${pacote}"
     }else if (isNode){ 
          sh " npm run build -f ${pacote}"
     }
}

return this
