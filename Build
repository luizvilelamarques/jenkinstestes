def Build(buildInfo){
     if (buildInfo.tipoProjetoMaven){
         BuildMaven(buildInfo.package)
     }else if (buildInfo.tipoProjetoNode){ 
         BuildNode(buildInfo.package)
     }
}

def BuildNode(packagejson) {
     sh " npm run build -f ${packagejson}"
}

def BuildMaven(filepom) {
     sh " mvn clean install -f ${filepom}"
}

return this
