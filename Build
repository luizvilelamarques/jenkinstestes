class BuildMaven {
     def package
     
     def build(){
           sh " mvn clean install -f ${package}"
     }
}

class BuildNodeJS {
     def package
     
     def build(){
           sh " npm run build -f ${package}"
     }
}

def instancia(buildInfo){
     if (buildInfo.tipoProjetoMaven){
         return new BuildMaven(package: buildInfo.package)
     }else if (isNode){ 
         return new BuildNodeJS(package: buildInfo.package)
     }
}

return this
