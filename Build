class BuildMaven {
     def pacote
     
     def build(){
           sh " mvn clean install -f ${pacote}"
     }
}

class BuildNodeJS {
     def package
     
     def build(){
           sh " npm run build -f ${pacote}"
     }
}

def instancia(buildInfo){
     if (buildInfo.tipoProjetoMaven){
         return new BuildMaven(pacote: buildInfo.package)
     }else if (isNode){ 
         return new BuildNodeJS(pacote: buildInfo.package)
     }
}

return this
