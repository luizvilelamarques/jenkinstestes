def pacote
def isMaven
def isNode

def instancia(buildInfo){
     isMaven= buildInfo.tipoProjetoMaven
     isNode = buildInfo.tipoProjetoNode
     pacote = buildInfo.package
     return this
}

def build(){
     if (isMaven){
         sh " mvn clean install -f ${pacote}"
     }else if (isNode){ 
          sh " npm run build -f ${pacote}"
     }
}

return this
