def Sonnar(filepom, sonarInfo){
    scannerHome = tool 'Sonar TJMG'
    POM_VERSION = sh script: "mvn -f ${filepom} help:evaluate -Dexpression=project.version -q -DforceStdout", returnStdout: true

    withSonarQubeEnv('SonarQube TJMG'){ 
        sh "${scannerHome}/bin/sonar-scanner -e -Dsonar.projectKey=${sonarInfo.projectKey} -Dsonar.projectVersion=${POM_VERSION} -Dsonar.java.source=1.8 -Dsonar.sources=${sonarInfo.sources} -Dsonar.java.binaries=**/target -Dsonar.sourceEncoding=ISO-8859-1"
    }
}

return this
