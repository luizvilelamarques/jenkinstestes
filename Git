class Git {
    def credentials
    def url
    def branch
    
    @NonCPS
    def checkout() {
        git branch: branch, credentialsId: credentials, url: url
    }
}


def ref    
    
def instancia(gitInfo, branch){
  ref = new Git(credentials: gitInfo.credentials, url: gitInfo.url, branch: branch)
  return this
}

def checkout() {
   ref.checkout()
}

return this
