def pacote
def isMaven
def isNode

def instancia(buildInfo){
     isMaven= buildInfo.tipoProjetoMaven
     isNode = buildInfo.tipoProjetoNode
     pacote = buildInfo.package
     return this
}

def test(){
     if (isMaven){
          sh " echo test maven"
     }else if (isNode){ 
          sh " echo test nodejs"
     }
}

return this
