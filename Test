class TestMaven {
     def pacote
     
     def test(){
           sh " echo test maven"
     }
}

class TestNodeJS {
     def package
     
     def test(){
           sh " echo test nodejs"
     }
}

def instancia(buildInfo){
     if (buildInfo.tipoProjetoMaven){
         return new TestMaven(pacote: buildInfo.package)
     }else if (isNode){ 
         return new TestNodeJS(pacote: buildInfo.package)
     }
}

return this
